############################################################
# 
############################################################

# arguments: ($1: from) ($2: to)
define HPC_moveIfStaticallyLinked
  if file "$1" 2>&1 | $(GREP) -E -i -e 'static.*link' >/dev/null ; then \
    rm -f "$2" ;  \
    mv -f "$1" "$2" ;  \
  fi
endef

#############################################################################

install-exec-hook:
	$(call HPC_moveIfStaticallyLinked,$(DESTDIR)$(pkglibexecdir)/hpcprof-bin$(EXEEXT),$(DESTDIR)$(bindir)/hpcprof$(EXEEXT))

%.cpp.pp : %.cpp
	$(CXXCPP) $(MYCPPFLAGS_0_CXX) $< > $@

%.c.pp : %.c
	$(CXXCPP) $(MYCPPFLAGS_0_CC)  $< > $@

